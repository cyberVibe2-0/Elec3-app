/* ################### Mainmenubar window #################### */

QMenuBar {
	background-color: #b1b1b1;
	left:5px;
}

QMenuBar::item {
	spacing: 10px; /* spacing between menu bar items */
     padding: 7px 10px;
	background: #b1b1b1;
	color:rgb(51,51,51);
	background-image:none;

}

QMenuBar::item:selected {
	background: #666;
	color:#fff;
	background-image:none;

}

QMenuBar::item:pressed {
	background: #666;
	color:#fff;
	background-image:none;

}

/*########### style of the dropdowns ######*/

QMenu{
	background-color: #b1b1b1;
	color:#333;
	margin: 0px 3px 0px 3px;
	border:none;
}

QMenu::item {
	background-color: #b1b1b1;
	color:#333;
	padding: 2px 15px 2px 15px;
}

QMenu::item:selected {
	background-color: #666;
	color:#fff;
}

 QMenu::separator {
	background-color: #919191;
	height:2px;
} 

 QMenu::indicator {
     width: 15px;
     height: 15px;
 }

QMenu::item:disabled {
	background-color: #888;
	color:#dadada;

}
QMenu::item:disabled:selected {
	background-color: #777;
	color:#dadada;

}

 /*############## indicator:non-exclusive is a checkbox ###########*/
 QMenu::indicator:non-exclusive:checked {
     image: url(:/resources/images/icons/check.png);
	 	 background-color:transparent;
		 	border:none;
 }
 
 QMenu::indicator:non-exclusive:checked:selected {
     image: url(:/resources/images/icons/checkhover.png);
	 	 background-color:transparent;
		 	border:none;
 }

 QMenu::indicator:non-exclusive:unchecked {  /*the checkbox for the mainmenu */
    image: url(:/resources/images/icons/nocheck.png);
	 	 background-color:#b1b1b1;
		 	border:transparent;
			
 }

  QMenu::indicator:non-exclusive:unchecked:selected {
     image: url(:/resources/images/icons/check.png);
	 	 background-color:#666;
		 	border:none;
 }
 
 
/*########### MAIN WINDOW TABBAR #################*/
	
 
#sketch_tabs::pane { /*the tab-window*/
 	padding: 0px 0px 0px 0px;
	border:none;
	background:#d5e1e9;
	margin:0px;
}


#sketch_tabs::tab-bar { /*this is the complete tab-bar*/
 	top: 0px;
 	left: 10px;
	right: 0px;
	
}

#sketch_tabs QTabBar#WelcomeView {
	image-position: left;
	image: url(:/resources/images/icons/arrowButtonRight.png);
	background-image: url(:/resources/images/icons/arrowButtonLeft.png);
}

#sketch_tabs QTabBar::tab  {
    margin: 0px 10px -1px 0px;
	min-width: 6.5em;
    max-height: 35px;
	height:35px;
	
  /*     padding-top: 2px;
        padding-bottom: 2px;
        padding-left: 10px;
        padding-right: 10px; */
	
}

#sketch_tabs QTabBar::tab:selected { /*the active tab*/
	font-family: "Droid Sans";
	font-size:14px;
	background-color: transparent;
	font-weight: regular;
	color: #fff;
	border:none;
	margin-bottom:-1px;
	background-image: url(:/resources/images/icons/segmentedSwitcherXActiveFocus.png);
}

#sketch_tabs QTabBar::tab:selected:hover { /*the active tab hover*/
	font-family: "Droid Sans";
	font-size:14px;
	background-color: transparent;
	font-weight: regular;
	color: #dadada;
	border:none;
	margin-bottom:-1px;
	background-image: url(:/resources/images/icons/segmentedSwitcherXActiveFocusHover.png);
}

#sketch_tabs QTabBar::tab:!selected { /*the inactive tabs*/
	background-color: transparent;
	font-family: "Droid Sans";
	font-size:14px;
	color: #666;
	font-weight:regular;
	margin-bottom:-1px;
	background-image: url(:/resources/images/icons/segmentedSwitcherXInactiveFocusHover.png);
}

#sketch_tabs QTabBar::tab:!selected:hover {  /*the inactive tab- hover*/
	background-color: transparent;
	font-family: "Droid Sans";
	font-size:14px;
	border:none;
	color: #444;
	font-weight:regular;
	margin-bottom:-1px;
	background-image: url(:/resources/images/icons/segmentedSwitcherXInactiveFocusHover.png);
}

