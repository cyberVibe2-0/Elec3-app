/* ################### zoomslider #################### */

QSlider::groove:horizontal { /*this is the slider - "line" */
	background:rgb(214,50,34);
	height: 2px;
	border-radius:-5px;
	margin-bottom:10px;
	margin-top:0px;
}


QSlider::handle:horizontal { /*this is the handle: it needs negative margin to be "bigger" than the slider - "line" */
	width:10px; 
	height:10px;
	margin-top: -4px;
	margin-bottom: -4px;
	background-clip: content;
	background-image: url(:/resources/images/icons/zoomSlider.png);

}

QSlider::handle:horizontal:hover {
	width:10px;
	height:10px;
	margin-top: -4px;
	margin-bottom: -4px;
	background-image: url(:/resources/images/icons/zoomSliderHover.png);

}


