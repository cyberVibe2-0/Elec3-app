/* ########### the main window and general things ############## */

MainWindow {
        /* layout here  */
		background-color:rgb(217,217,217); /*mainbackground color*/
        /* styling there */
        font-size: 11px;
        color: rgb(255,255,255);
}


QStatusBar {
        background-color: rgb(130,31,9); /* the statusbar at the bottom */
		color: rgb(255,255,255);
}

#statusBarContainer {
        background-color: rgb(130,31,9);/* the statusbar at the bottom */
		color: rgb(255,255,255);
}

#statusBarContainer QTooltip{
     border: 1px solid #fff;
     padding: 3px;
     border-radius: 1px;
	 background:#fff;
	 color:#333;
	  opacity: 200;
}

QScrollBar:vertical{               
         border: 0px; 
         background:rgba(255,255,255,10); 
         width:10px;     
         margin: 0px 0px 0px 0px; 
     } 
	 
QScrollBar::handle:vertical{ 
		border: 2px solid #666;
		border-radius: 2px;
         background: #666;
         min-height: 0px; 
      
     }
	 
QScrollBar::add-line:vertical{ 
         background: #fff; 
         height: 10px; 
         subcontrol-position: bottom; 
         subcontrol-origin: margin; 
     }
	 
QScrollBar::sub-line:vertical{ 
         background:#fff; 
         height: 10px; 
         subcontrol-position: top; 
         subcontrol-origin: margin; 
     } 
	 

QScrollBar:horizontal
{
        border: 0px; 
         background:rgba(255,255,255,10); 
         height:10px;     
         margin: 0px 0px 0px 0px; 
	}

QScrollBar::handle:horizontal
{
			border: 2px solid #666;
		border-radius: 2px;
         background: #666;
         min-width: 0px; 
	}
 
QScrollBar::add-line:horizontal
{
	         background: #fff; 
         width: 10px; 
subcontrol-position: right;
subcontrol-origin: margin;
}
 
QScrollBar::sub-line:horizontal
{
	         background:#fff; 
         width: 10px; 
	subcontrol-position: left;
subcontrol-origin: margin;
	}


/* this separates palettes from each other and is movable */
MainWindow::separator {
